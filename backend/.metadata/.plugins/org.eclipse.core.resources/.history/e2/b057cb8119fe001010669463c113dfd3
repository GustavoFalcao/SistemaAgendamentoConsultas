public class Paciente {
    private String nome;
    private String cpf;

    public Paciente(String nome, String cpf) {
        this.nome = nome;
        this.cpf = cpf;
    }

    public String getNome() {
        return nome;
    }

    public String getCpf() {
        return cpf;
    }

    public void alterarNome(String novoNome) {
        this.nome = novoNome;
    }

    @Override
    public String toString() {
        return "Paciente: " + nome + " | CPF: " + cpf;
    }
}
